    private void jTable1MouseReleased(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_jTable1MouseReleased

        //pintar celula ao clicar
        int linha = getJTable1().rowAtPoint(evt.getPoint());
        int coluna = getJTable1().columnAtPoint(evt.getPoint());
        System.out.println("Coluna: "+linha+"Linha: "+coluna);
        //cRenderer.colorirCelula(linha, coluna, Color.black);
        if(!ultimoClicadoB){
            ultimoClicado.clear();
            ultimoClicado.add(linha);
            ultimoClicado.add(coluna);
        }
        if(!primeiroClicadoB){
            primeiroClicado.clear();
            primeiroClicado.add(linha);
            primeiroClicado.add(coluna);
        }
        System.out.println("ultX:"+ultimoClicado.get(0).toString()+" ultY: "+ultimoClicado.get(1).toString());
        if(evt.isPopupTrigger()){
            jPopupMenu1.show(evt.getComponent(), evt.getX(), evt.getY());
        }
        getJTable1().repaint();
    }//GEN-LAST:event_jTable1MouseReleased


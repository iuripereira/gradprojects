    private void paintBariers(CellRederizador cRenderer) {
        //throw new UnsupportedOperationException("Not yet implemented");
        if(!map.isEmpty()){
            for (int a = 0; a < map.size(); a++) {
                String[] str = (String[]) map.get(a);
                for (int b = 0; b < str.length; b++) {
                    if(Integer.parseInt(str[b])==1){
                        cRenderer.colorirCelula(a, b, new Color(0x2D,0x2F,0x26));//#2D2F26
                    }
                }

            }
        }
    }

    private void cleanPaint(){
        for (int i = 0; i < map.size(); i++) {
            String[] str = (String[]) map.get(i);
            for (int j = 0; j < str.length; j++) {
                cRenderer.colorirCelula(i, j, new Color(0xFF,0xFF,0xFF));
            }
        }
        ultimoClicado.clear();
        ultimoClicadoB=false;
        primeiroClicado.clear();
        primeiroClicadoB=false;
    }

    private void colorirFinal(){
        if(!ultimoClicadoB == true){
            cRenderer.colorirCelula(Integer.parseInt(ultimoClicado.get(0).toString()),
                    Integer.parseInt(ultimoClicado.get(1).toString()),
                    new Color(0xFF,0xDA,0x2F));//#FFDA2F
        }
    }
    private void colorirInicial(){
        if(!primeiroClicadoB == true){
            cRenderer.colorirCelula(Integer.parseInt(ultimoClicado.get(0).toString()),
                    Integer.parseInt(ultimoClicado.get(1).toString()),
                    new Color(0x67,0xC7,0xCF));//##67C7CF
        }
    }
